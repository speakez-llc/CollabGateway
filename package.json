{
    "type": "module",
    "private": true,
    "scripts": {
        "prestart": "dotnet tool restore",
        "start": "dotnet fable watch ./src/CollabGateway.Client --outDir ./src/CollabGateway.Client/.fable-build --run vite",
        "start:prod": "vite preview --config /CollabGateway/vite.config.js --port 8080",
        "prebuild": "dotnet tool restore",
        "build": "dotnet fable ./src/CollabGateway.Client --outDir ./src/CollabGateway.Client/.fable-build && sh -c \"node --stack-size=65500 node_modules/vite/bin/vite build --emptyOutDir\""
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.19",
        "fable-compiler": "^2.13.0",
        "fable-splitter": "^0.1.21",
        "postcss": "^8.4.38",
        "rollup": "^4.23.0",
        "vite": "^5.2.6",
        "cross-env": "^7.0.3"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.6.0",
        "daisyui": "^4.9.0",
        "pigeon-maps": "^0.21.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recharts": "^2.13.3",
        "tailwindcss": "^3.4.1",
        "use-sync-external-store": "^1.2.0",
        "wrangler": "^3.78.6"
    }
}